{
  "manifest_version": 2,
  "name": "AutoZy",
  "version": "0.3",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://learn.zybooks.com/*"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}